@model PlataformaIEB.Models.Paciente
@{
    ViewBag.Title = "Cadastro de Paciente";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<div class="row">
<div class="col-md-offset-1">
    @using (Html.BeginForm())
    { 
        
        @Html.Partial("_FormPaciente",Model)


        <div class="form-group">
            <div class="row">
                <div class="col-sm-1">
                    @Html.LabelFor(m => m.Senha)
                </div>
                <div class="col-sm-1">
                    @Html.EditorFor(m => m.Senha)
                </div>
                <span id="helpBlock2" class="help-block">
                    @Html.ValidationMessageFor(m => m.Senha)
                </span>
            </div>
        </div>

        <div class="form-group">
            <div class="row">
                <input type="submit" class="btn btn-success" value="Cadastrar" />
                @Html.ActionLink("Voltar", "PrincipalPaciente", new { area = "Admin", Controller = "Cadastro" }, new { @class = "btn btn-danger" })
            </div>
        </div>
    }
</div>
</div>


