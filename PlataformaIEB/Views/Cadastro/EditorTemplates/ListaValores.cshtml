@model PlataformaIEB.Models.ListaValores

@Html.HiddenFor(model => model.ConsultaID)

@Html.HiddenFor(model => model.VariavelID)


<div class="form-group">
    @Html.Label(Model.Variavel.Nome, new { @class = "control-label col-lg-10" })
    <div class=" col-md-10">
        @Html.EditorFor(m => m.Valor, new { @class = "form-control" })
        <span class="help-block">
            @Html.ValidationMessageFor(m => m.Valor, "", new { @class = "text-danger" })
        </span>
    </div>
</div>


   



